
---
import BaseHead from "../components/BaseHead.astro";
import Content from "../components/Content.astro";
import Header from "../components/Header.astro";
import Masthead from "../components/Masthead.astro";
import Footer from "../components/Footer.astro";
import { IoMdArrowForward } from "react-icons/io";
import { SITE_TITLE, SITE_DESCRIPTION } from "../consts";
import Body from "../components/Body.astro";
import { IoChevronForward } from "react-icons/io5";

---

<!doctype html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
  </head>
  <Body>
    <Header title={SITE_TITLE} />
    <main class="pt-[56px]">
      <Masthead />
      <Content>
        <section>
          <h1 class="text-3xl text-center font-mplus mt-12 mb-8">Opss ... No funciona</h1>
          <div class="text-center py-4">
            <a class="inline-flex items-center gap-1 underline underline-offset-2 decoration-orange-500" href="/"> Regresar <IoChevronForward></IoChevronForward></a>
          </div>
        </section>
      </Content>
    </main>
    <Footer />
  </Body>
</html>
